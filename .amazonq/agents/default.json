{
  "name": "default-agent",
  "version": "1.0.0",
  "description": "Default agent configuration",
  "mcpServers": {
    "memory": {
      "command": "node",
      "args": [
        "node_modules/@modelcontextprotocol/server-memory/dist/index.js"
      ],
      "disabled": false
    },
    "context7-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@upstash/context7-mcp",
        "--key",
        "fc1c0930-c457-4042-8088-e052f8dff806",
        "--profile",
        "loose-goat-4drgNz"
      ],
      "disabled": false
    },
    "eslint": {
      "command": "npx",
      "args": [
        "@eslint/mcp@latest"
      ],
      "disabled": false
    }
  },
  "tools": [
    "fsRead",
    "fsWrite",
    "fsReplace",
    "listDirectory",
    "fileSearch",
    "executeBash",
    "codeReview",
    "displayFindings",
    "@memory",
    "@context7-mcp",
    "@eslint"
  ],
  "allowedTools": [
    "fsRead",
    "listDirectory",
    "fileSearch",
    "codeReview",
    "displayFindings",
    "fsWrite",
    "fsReplace",
    "@context7-mcp/resolve-library-id",
    "@context7-mcp/get-library-docs",
    "@eslint/lint-files"
  ],
  "toolsSettings": {
    "execute_bash": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    },
    "use_aws": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    }
  },
  "includedFiles": [],
  "resources": [],
  "promptHooks": []
}