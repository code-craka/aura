{
  "name": "@project-aura/ai-engine",
  "version": "0.1.0",
  "description": "AI engine package for Project Aura - handles AI model routing, context management, and intelligent processing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.mjs",
      "require": "./dist/models/index.js"
    },
    "./processing": {
      "types": "./dist/processing/index.d.ts",
      "import": "./dist/processing/index.mjs",
      "require": "./dist/processing/index.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.mjs",
      "require": "./dist/agents/index.js"
    },
    "./privacy": {
      "types": "./dist/privacy/index.d.ts",
      "import": "./dist/privacy/index.mjs",
      "require": "./dist/privacy/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:prod": "tsup --minify",
    "dev": "tsup --watch",
    "dev:full": "tsup --watch",
    "test": "vitest",
    "test:unit": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest --config vitest.performance.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "openai",
    "anthropic",
    "google-ai",
    "context-management",
    "privacy",
    "typescript"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@google/generative-ai": "^0.14.0",
    "@pinecone-database/pinecone": "^2.0.1",
    "@project-aura/shared-utils": "file:../shared-utils",
    "crypto-js": "^4.2.0",
    "ollama": "^0.4.0",
    "openai": "^4.24.7",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "markdownlint-cli": "^0.45.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/project-aura.git",
    "directory": "packages/ai-engine"
  },
  "bugs": {
    "url": "https://github.com/your-org/project-aura/issues"
  },
  "homepage": "https://github.com/your-org/project-aura/tree/main/packages/ai-engine#readme"
}
