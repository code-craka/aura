{
  "name": "@project-aura/browser-core",
  "version": "0.1.0",
  "description": "Browser core package for Project Aura - Chromium-based browser engine with AI integration and process management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "import": "./dist/engine/index.mjs",
      "require": "./dist/engine/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.mjs",
      "require": "./dist/ui/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:prod": "tsup --minify",
    "dev": "tsup --watch",
    "dev:full": "tsup --watch",
    "test": "vitest",
    "test:unit": "vitest",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest --config vitest.performance.config.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "browser",
    "chromium",
    "ai",
    "typescript",
    "process-management",
    "tab-management",
    "security"
  ],
  "dependencies": {
    "events": "^3.3.0",
    "@project-aura/ai-engine": "workspace:*",
    "@project-aura/shared-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.1",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/project-aura.git",
    "directory": "packages/browser-core"
  },
  "bugs": {
    "url": "https://github.com/your-org/project-aura/issues"
  },
  "homepage": "https://github.com/your-org/project-aura/tree/main/packages/browser-core#readme"
}
